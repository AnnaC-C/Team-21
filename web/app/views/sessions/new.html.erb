<%= form_for(:session, url: login_path) do |f| %>

  <%= f.label :email %>
  <%= f.email_field :email %>
  <br /><br />

  <%= f.label :password %>
  <%= f.password_field :password %>
  <br /><br />

  <%= f.submit "Log in" %>
  <br /><br />
  <p>Don't have an account? <%= link_to "Sign up!", signup_path %>.</p>
<% end %>
